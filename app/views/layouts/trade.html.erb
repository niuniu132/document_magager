<!DOCTYPE html>
<html>
  <head>
    <title>雅安市商业银行运营管理平台</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class='container-fluid'>
        <%= render 'common/navbar' %>
        <%= render 'common/flashes' %>
        <div class="row">
            <div class="col-md-2">
                <ul class="nav nav-pills nav-stacked" style="max-width: 300px; padding-top: 20px;">
                    <li class="active"><%= link_to("前台交易",'#', class:'btn btn-primary')%></li>
                    <li class="active"><%= link_to("银联交易",'#', class:'btn btn-primary')%></li>
                    <li class="active"><%= link_to("支付系统交易",'#', class:'btn btn-primary')%></li>
                </ul>
            </div>
            <div class="col-md-10">
                <%= yield%>
            </div>


        </div>
    <%= render 'common/footer'%>
  </body>

</html>
